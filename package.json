{
  "name": "myapp",
  "version": "0.0.0",
  "license": "MIT",
  "main": "dist/apps/firebase-functions/main.js",
  "engines": {
    "node": "16"
  },
  "scripts": {
    "postinstall": "ngcc --properties es2020 browser module main",
    "clean": "rm -rf ./dist/*",
    "init:osx": "./tools/scripts/install.osx.sh",
    "init:linux": "./tools/scripts/install.linux.sh",
    "init:windows": "./tools/scripts/install.windows.ps1",
    "docs:gen:images": "cd docs/diagrams && python diagram.py",
    "_run:test:coverage:clean": "mkdir -p ./coverage && rm -rf ./coverage/*",
    "_run:test:coverage:exec": "nx run-many --target=test --codeCoverage --coverageReporters=lcovonly,text,text-summary --parallel --skip-nx-cache",
    "_run:test:coverage:merge": "ts-node ./tools/scripts/coverageMerge.ts",
    "_run:test:coverage:generate": "genhtml ./coverage/lcov.info -o coverage",
    "_run:test:coverage:echo": "echo Open Coverage Report in Browser: file://$(pwd)/coverage/index.html ",
    "test:coverage": "run-s _run:test:coverage:clean _run:test:coverage:exec _run:test:coverage:merge _run:test:coverage:generate _run:test:coverage:echo",
    "build": "nx run-many --target=build",
    "start:local": "run-s build firebase:emulate",
    "firebase:emulate": "cross-env USE_FIREBASE_EMULATOR=true firebase emulators:start --project demo-firebase-project -c ./firebase.json --inspect-functions",
    "firebase:emulate:functions": "cross-env USE_FIREBASE_EMULATOR=true firebase emulators:start --project demo-firebase-project -c ./firebase.json --only functions --inspect-functions",
    "firebase:emulate:stop": "lsof -t -i:5001 -i:5002 -i:5002 -i:5003 -i:5004 -i:5005 -i:5006 -i:5007 -i:5008 -i:9595 -i:9229 -i:4400 -i:4500 | xargs kill -9",
    "firebase:deploy:functions": "firebase use dev && firebase deploy --only functions",
    "script:startNgrok": "ts-node ./tools/scripts/startNgrok.ts",
    "terraform": "terraform -help",
    "terraform:init": "terraform -chdir=./deployment/tf init",
    "terraform:fmt": "terraform -chdir=./deployment/tf fmt",
    "terraform:validate": "terraform -chdir=./deployment/tf validate",
    "terraform:check": "run-s terraform:init terraform:validate terraform:fmt",
    "terraform:graph": "terraform -chdir=./deployment/tf graph -draw-cycles -type=plan | dot -Tsvg > ./deployment/tf/graph.svg",
    "terraform:plan": "cross-env-shell terraform -chdir=./deployment/tf plan -var-file='./env/$APP_DEPLOYMENT_ENV.tfvars'",
    "terraform:plan:dev": "cross-env-shell APP_DEPLOYMENT_ENV=dev run-s terraform:plan",
    "terraform:apply": "cross-env-shell terraform -chdir=./deployment/tf plan -var-file='./env/$APP_DEPLOYMENT_ENV.tfvars'",
    "terraform:apply:dev": "cross-env-shell APP_DEPLOYMENT_ENV=dev run-s terraform:apply"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~14.1.1",
    "@angular/common": "~14.1.1",
    "@angular/compiler": "~14.1.1",
    "@angular/core": "~14.1.1",
    "@angular/forms": "~14.1.1",
    "@angular/platform-browser": "~14.1.1",
    "@angular/platform-browser-dynamic": "~14.1.1",
    "@angular/router": "~14.1.1",
    "@google-cloud/logging": "^10.1.4",
    "cross-env": "^7.0.3",
    "firebase-admin": "^11.0.1",
    "firebase-functions": "^3.22.0",
    "npm-run-all": "^4.1.5",
    "rxjs": "~7.4.0",
    "ts-node": "10.9.1",
    "tslib": "^2.3.0",
    "uuid": "^8.3.2",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~14.1.1",
    "@angular-eslint/eslint-plugin": "~14.0.0",
    "@angular-eslint/eslint-plugin-template": "~14.0.0",
    "@angular-eslint/template-parser": "~14.0.0",
    "@angular/cli": "~14.1.1",
    "@angular/compiler-cli": "~14.1.1",
    "@angular/language-service": "~14.1.1",
    "@apideck/postman-to-k6": "^1.8.5",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.18.9",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-typescript": "^7.18.6",
    "@nrwl/angular": "^14.5.6",
    "@nrwl/cli": "14.5.6",
    "@nrwl/cypress": "14.5.6",
    "@nrwl/eslint-plugin-nx": "14.5.6",
    "@nrwl/jest": "14.5.6",
    "@nrwl/js": "14.5.6",
    "@nrwl/linter": "14.5.6",
    "@nrwl/node": "^14.5.6",
    "@nrwl/workspace": "14.5.6",
    "@types/express": "^4.17.13",
    "@types/jest": "27.4.1",
    "@types/k6": "^0.37.1",
    "@types/node": "16.11.7",
    "@types/uuid": "^8.3.4",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "babel-loader": "^8.2.5",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "cypress": "^10.2.0",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "jest": "^28.1.3",
    "jest-preset-angular": "~11.1.2",
    "ngrok": "^4.3.3",
    "nx": "^14.5.6",
    "prettier": "^2.6.2",
    "shelljs": "^0.8.5",
    "ts-jest": "27.1.4",
    "typescript": "^4.7.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-glob-entries": "^1.0.1"
  }
}
